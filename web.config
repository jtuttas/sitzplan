<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="iisnode" path="dist/app.js" verb="*" modules="iisnode" />
        </handlers>   
        <rewrite>
      <rules>
        <rule name="myapp">
          <match url="/*" />
          <action type="Rewrite" url="dist/app.js" />
        </rule>
      </rules>
    </rewrite>         
    </system.webServer>
</configuration>